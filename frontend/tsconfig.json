{
    "extends": "@finsweet/tsconfig",
    "compilerOptions": {
        "rootDir": ".",
        "baseUrl": "./",
        "paths": {
            "$utils/*": ["src/utils/*"]
        },
        "types": ["@finsweet/ts-utils", "@stripe/stripe-js"],
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
    }
}
